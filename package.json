{
    "name": "ng2-dg",
    "displayName": "ng2-dependency-graph",
    "description": "generate a Angular2 dependency graph",
    "version": "0.0.2",
    "publisher": "web-dave",
    "engines": {
        "vscode": "^1.0.0"
    },
    "bugs": {
        "url": "https://github.com/web-dave/ng2-dg/issues"
    },
    "homepage": "https://github.com/web-dave/ng2-dg/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/web-dave/ng2-dg.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ng2dgt"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.ng2dgt",
                "title": "ng2-dg tsconfig"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "angular2-dependencies-graph": "^0.2.6"
    }
}